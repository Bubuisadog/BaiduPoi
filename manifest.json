{
  "manifest_version": 2,
  "name": "BaiduPoi",
  "description": "BaiduPoi",
  "version": "0.1",
  "browser_action": {
    "default_icon": "assets/images/icon--disable.png",
    "default_title": "BaiduPoi"
  },
  "icons": {"48": "assets/images/icon.png"},
  "background": {
    "page": "main.html"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "storage"
  ]
}